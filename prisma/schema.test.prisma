generator client {
    provider = "prisma-client-js"
}


datasource db {
    provider = "mysql"
    url = env("DATABASE_TEST_URL")
    
}





model Product {
  id String @id @default(uuid())
  name String
  price Float
  stock Stock[]
}

model Stock {
    id String @id @default(uuid())
    product Product @relation(fields: [productId],references: [id])
    productId String
    quantity Int
}