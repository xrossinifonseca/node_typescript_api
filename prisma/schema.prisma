

generator client {
    provider = "prisma-client-js"
}


datasource db {
    provider = "mysql"
    url = env("DATABASE_URL")
    
}



model Store {
    id String @id @default(uuid())
    name String
    region String

    @@map("stores")

}

model Product {
  id String @id @default(uuid())
  name String
  lotNumber String
  qty Int
  price Decimal @db.Decimal(10,2)
  validity DateTime? @db.Timestamp(0)
}